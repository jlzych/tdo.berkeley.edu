%h1 Create New User

= form_for(@user, :url => create_user_path) do |f|
  -#= devise_error_messages!

  %p
    =f.label :email
    =f.email_field :email

  %p
    =f.label :password
    =f.password_field :password

  %p
    =f.label :password_confirmation
    =f.password_field :password_confirmation

  %p= f.submit "Sign up"
